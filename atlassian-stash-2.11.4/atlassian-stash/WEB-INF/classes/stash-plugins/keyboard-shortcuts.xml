<atlassian-plugin key="com.atlassian.stash.stash-keyboard-shortcuts" name="System Keyboard Shortcuts">

    <!-- Global 100 -->

    <keyboard-shortcut key="shortcut-close-shortcuts-dialog" i18n-name="keyboard.shortcut.close.shortcuts.dialog" name="Close Shortcuts Dialog">
        <description key="keyboard.shortcut.close.shortcuts.dialog.desc">Close dialog</description>
        <shortcut>[esc]</shortcut>
        <operation type="evaluate">/* noop - handled by AUI Dialog */</operation>
        <order>110</order>
        <context>global</context>
    </keyboard-shortcut>

    <keyboard-shortcut key="shortcut-open-shortcuts-dialog" i18n-name="keyboard.shortcut.open.shortcuts.dialog" name="Open Shortcuts Dialog">
        <description key="keyboard.shortcut.open.shortcuts.dialog.desc">Show this dialog</description>
        <shortcut>?</shortcut>
        <operation type="execute">require('widget/keyboard-shortcuts').showDialog()</operation>
        <order>120</order>
        <context>global</context>
    </keyboard-shortcut>

    <keyboard-shortcut key="repository-search-shortcuts-dialog" i18n-name="keyboard.shortcut.repository.search" name="Repository Search">
        <description key="keyboard.shortcut.repository.search.desc">Find a repository</description>
        <shortcut>/</shortcut>
        <operation type="moveToAndFocus">#repository-search</operation>
        <order>125</order>
        <context>global</context>
    </keyboard-shortcut>

    <keyboard-shortcut key="shortcut-recent-repositories" i18n-name="keyboard.shortcut.recent.repositories" name="Recent Repositories">
        <description key="keyboard.shortcut.recent.repositories.desc">Recent repositories</description>
        <shortcut>gr</shortcut>
        <operation type="click">#repositories-menu-trigger</operation>
        <order>130</order>
        <context>global</context>
    </keyboard-shortcut>

    <keyboard-shortcut key="shortcut-megazord" i18n-name="keyboard.shortcut.what.time.is.it" name="Alert Morphin' Time" hidden="true">
        <description key="keyboard.shortcut.what.time.is.it.desc">What time is it?</description>
        <shortcut>megazord</shortcut>
        <operation type="execute">alert("It's Morphin' Time!")</operation>
        <order>190</order>
        <context>global</context>
    </keyboard-shortcut>

    <!-- Repository 200 -->

    <keyboard-shortcut key="shortcut-branch-selector" i18n-name="keyboard.shortcut.branch.selector" name="Open Branch Selector">
        <description key="keyboard.shortcut.branch.selector.desc">Change branch/tag</description>
        <shortcut>b</shortcut>
        <operation type="click">#repository-layout-revision-selector</operation>
        <order>210</order>
        <context>branch</context>
    </keyboard-shortcut>

    <!-- Changeset 300 -->

    <keyboard-shortcut key="shortcut-changeset-post-comment" i18n-name="keyboard.shortcut.changeset.post.comment" name="Changeset - Submit Comment">
        <description key="keyboard.shortcut.comments.post.desc">Post comment</description>
        <shortcut>[ctrl+return]</shortcut>
        <operation type="evaluate">/* noop - handled by comment forms */</operation>
        <order>310</order>
        <context>changeset</context>
    </keyboard-shortcut>

    <!-- If you modify this, please update markup-preview.js as well -->
